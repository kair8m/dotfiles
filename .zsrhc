export ZSH="$HOME/.oh-my-zsh"
ZSH_THEME="crunch"
plugins=(
	git
	zsh-autosuggestions
)

export PATH="/usr/local/opt/python@3.10/libexec/bin:$PATH"
alias py=python
alias sc"=source ~/.zshrc"
alias top=vtop
alias rm=trash
alias cat=bat

source $ZSH/oh-my-zsh.sh

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh
